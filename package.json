{
  "name": "getlabs-api",
  "version": "0.0.1",
  "description": "GetLabs API",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^14.15.1",
    "yarn": "^1.19"
  },
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- --config ./src/ormconfig.ts",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "build": "nest build",
    "clean": "rimraf ./dist",
    "cli": "node ./dist/src/cli.js",
    "cli:debug": "node --inspect=0.0.0.0:9222 ./dist/cli.js",
    "db": "cd ./dist/src && node ../../node_modules/.bin/typeorm",
    "db:proxy": "./extra/cloud_sql_proxy-macos -instances=getlabs-prod:us-west2:getlabs-db-master-614d=tcp:54320",
    "db:proxy:staging": "./extra/cloud_sql_proxy-macos -instances=getlabs-staging-1:northamerica-northeast1:getlabs-db-master-1371=tcp:54321",
    "docker-compose:env": "DOCKER_BUILDKIT=1 BUILDKIT_PROGRESS=plain VERSION=$(git rev-parse HEAD) TAG=$(git rev-parse --short HEAD) docker-compose -f docker-compose.build.yml",
    "docker:build": "yarn docker-compose:env build api",
    "docker:push": "yarn docker-compose:env push api",
    "docker:run": "yarn docker-compose:env run --service-ports api",
    "docker:exec": "docker-compose exec api yarn run",
    "docker:restart": "yarn docker:stop && yarn docker:start",
    "docker:start": "chmod 600 docker/router/acme.json && docker-compose up -d proxy api && docker-compose logs -f -t --tail=0",
    "docker:stop": "docker-compose stop",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn run lint --fix",
    "start": "nest start",
    "start-worker": "nest start worker",
    "start:dev": "nest start --watch --watchAssets --preserveWatchOutput --debug 0.0.0.0:9229",
    "start-worker:dev": "nest start worker --watch --watchAssets --preserveWatchOutput --debug 0.0.0.0:9228",
    "start:prod": "node ./dist/src/main",
    "start-worker:prod": "node ./dist/src/worker",
    "start:hmr": "yarn run clean && concurrently --kill-others --names \"webpack,nest\" \"yarn run webpack --watch\" \"yarn run webpack:server\"",
    "stripe:listen": "stripe listen --forward-to https://api.getlabs.io/webhook/stripe",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.config.js --runInBand",
    "test:watch": "jest --watch",
    "tunnel": "ngrok http -bind-tls=true -host-header=rewrite api.getlabs.io:443",
    "webpack": "webpack --config webpack.config.js",
    "webpack:server": "wait-on dist/webpack-server.js && node --inspect=0.0.0.0:9229 dist/webpack-server"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@google-cloud/pubsub": "^2.6.0",
    "@google-cloud/storage": "^5.5.0",
    "@google-cloud/trace-agent": "^5.1.1",
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.1.5",
    "@sentry/integrations": "^5.27.4",
    "@sentry/node": "^5.27.4",
    "@slack/web-api": "^6.0.0",
    "@types/bull": "^3.14.4",
    "@types/json2csv": "^5.0.1",
    "@types/mixpanel": "^2.14.2",
    "@types/node": "^12.19.6",
    "@types/passport-anonymous": "^1.0.3",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.10",
    "bcryptjs": "^2.4.3",
    "bull": "^3.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.4",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "date-holidays": "^1.8.3",
    "envalid": "^6.0.2",
    "faker": "^5.1.0",
    "file-type": "^16.0.1",
    "geojson": "^0.5.0",
    "glob": "^7.1.5",
    "glob-promise": "^3.4.0",
    "gm": "^1.23.1",
    "handlebars": "^4.7.6",
    "handlebars-layouts": "^3.1.4",
    "hellosign-sdk": "^1.6.0",
    "helmet": "^4.2.0",
    "inquirer": "^7.3.3",
    "ioredis": "^4.19.2",
    "jimp": "^0.16.1",
    "json2csv": "^5.0.4",
    "juice": "^7.0.0",
    "libphonenumber-js": "^1.9.4",
    "lodash": "^4.17.20",
    "mixpanel": "^0.13.0",
    "mixpanel-data-export-node": "^2.1.0",
    "nanoid": "^3.1.17",
    "nocache": "^2.1.0",
    "nodemailer": "^6.4.16",
    "p-iteration": "^1.1.7",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "puppeteer-core": "chrome-72",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "slug": "^4.0.2",
    "stream-splitter": "^0.3.2",
    "stripe": "^8.124.0",
    "timezones.json": "^1.5.2",
    "twilio": "^3.56.0",
    "typeorm": "^0.2.29",
    "typescript": "4.1.2",
    "uuid": "^8.3.1",
    "validator": "^13.1.17",
    "winston": "^3.3.3",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/testing": "^7.6.12",
    "@sentry/cli": "^1.59.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cls-hooked": "^4.3.1",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/geojson": "^7946.0.7",
    "@types/glob": "^7.1.3",
    "@types/gm": "^1.18.9",
    "@types/inquirer": "^7.3.1",
    "@types/ioredis": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.2",
    "@types/pdfjs-dist": "^2.1.6",
    "@types/puppeteer-core": "^1.9.0",
    "@types/randomstring": "^1.1.6",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-haste-map": "^26.6.2",
    "jest-resolve": "^26.6.2",
    "lint-staged": "^10.5.3",
    "loader-utils": "^2.0.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "wait-on": "^5.2.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,scss}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./libs",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "examples"
    ],
    "testEnvironment": "node"
  }
}
